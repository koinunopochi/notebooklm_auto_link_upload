{
    "manifest_version": 3,
    "name": "NotebookLM URL Uploader",
    "version": "1.0",
    "description": "URLをNotebookLMに自動アップロードする拡張機能",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "options_page": "options.html",
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
  }
